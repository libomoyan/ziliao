    <servlet-name>CustomerServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.CustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ServicerServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.ServicerServlet</servlet-class>
  </servlet>
  <servlet>
