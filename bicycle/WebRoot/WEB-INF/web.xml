    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>BikeServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.BikeServlet</servlet-class>
  </servlet>












 
  <servlet-mapping>
    <servlet-name>SearchPageServlet</servlet-name>
    <url-pattern>/SearchPageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminLogoutServlet</servlet-name>
    <url-pattern>/AdminLogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BikeSortPageServlet</servlet-name>
    <url-pattern>/BikeSortPageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>YuDingServlet</servlet-name>
    <url-pattern>/YuDingServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CustomerServlet</servlet-name>
    <url-pattern>/CustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServicerServlet</servlet-name>
    <url-pattern>/ServicerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BikeServlet</servlet-name>
    <url-pattern>/BikeServlet</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
