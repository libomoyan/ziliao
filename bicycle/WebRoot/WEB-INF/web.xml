  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AdminLogoutServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.AdminLogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>BikeSortPageServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.BikeSortPageServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>YuDingServlet</servlet-name>
    <servlet-class>com.zyl.bicycle.servlet.YuDingServlet</servlet-class>
  </servlet>

  <servlet>
    <description>This is the description of my J2EE component</description>
